{
  "name": "apprecio-survey-module",
  "version": "1.0.0",
  "description": "HubSpot Custom Module - Encuesta Apprecio",
  "main": "module.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "esbuild module.js --bundle --minify --outfile=assets/survey-bundle.min.js",
    "build:css": "sass module.scss assets/survey-styles.css --style=compressed"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "survey-core": "^1.12.17",
    "survey-react-ui": "^1.12.17"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "sass": "^1.69.0"
  }
}
